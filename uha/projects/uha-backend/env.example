# Application Mode
UHA_MODE=DEVELOPMENT

# Database Configuration
UHA_DB__URL=sqlite+aiosqlite:///./data/database.db
UHA_DB__ECHO=false

# Session Configuration
UHA_SESSION__SECRET_KEY=your-secret-key-here

# CORS Configuration
UHA_CORS__ALLOW_ORIGINS=["http://localhost:3000", "http://localhost:8000"]

# FastAPI Configuration
UHA_FASTAPI__TITLE="uha API"
UHA_FASTAPI__DESCRIPTION="A modern FastAPI project with clean architecture"

# Cache Configuration
UHA_CACHE__BACKEND_URL=redis://localhost:6379/0
UHA_CACHE__ENABLE=true
